#include<stdio.h>
int main()
{
    int a[3][3];
    int b[3][3];
    int row,col;
    printf("enter the matrix a : \n ");
    for(row=0;row<3;row++)
    {
        for(col=0;col<3;col++)
        {
            scanf("%d",&a[row][col]);
        }
    }
    for(row=0;row<3;row++)
    {
        for(col=0;col<3;col++)
        {
            b[col][row] = a[row][col];
        }

    }
    printf("the original matrix : \n ");
    for(row=0;row<3;row++)
    {
        for(col=0;col<3;col++)
        {
            printf("%d \t",a[row][col]);
        }
        printf("\n");
    }
    printf("the the transpose matrix is : \n");
    for(row=0;row<3;row++)
    {
        for(col=0;col<3;col++)
        {
            printf("%d\t",b[row][col]);
        }
        printf("\n");
    }
    return 0;

}
